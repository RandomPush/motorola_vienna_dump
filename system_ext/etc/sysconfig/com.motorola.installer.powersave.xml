<?xml version="1.0" encoding="utf-8"?>
<config>
    <!-- Installer service must be  white-listed to always have internet access while
         in power save mode and datasaver mode, even if it isn't in the foreground. -->

    <!-- Installer must always have network access for polling, downloading and installing  -->
    <allow-in-power-save package="com.motorola.installer" />
    <allow-in-data-usage-save package="com.motorola.installer" />
</config>
